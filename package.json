    "date-fns": "^2.30.0",
    "element-plus": "^2.4.4",
    "express": "^4.18.2",
    "google-auth-library": "^9.11.0",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.13",
    "vue": "^3.2.13",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.12.16",
